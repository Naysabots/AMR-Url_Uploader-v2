{
  "name": "AMR-Url_Uploader",
  "description": "An url uploader bot with permenent thumbnail support",
  "keywords": [
    "telegram",
    "url",
    "zee5",
    "downloader"
    "uploader",
    "pyrogram"
  ],
  
  "success_url": "https://github.com/Satyamurthi",
  "website": "https://github.com/Satyamurthi/AMR-Url_Uploader",
  "repository": "https://github.com/Satyamurthi/AMR-Url_Uploader",
  
  "env": {
    "WEBHOOK": {
      "description": "Setting this to ANYTHING will enable webhooks when in env mode",
      "value": "ANYTHING"
    },
    "BOT_TOKEN": {
      "description": "Your bot token from @botfather, as a string."
      "value": ""
    },
    "TG_BOT_TOKEN": {
      "description": "Your bot token, as a string.",
      "value": ""
    },
    "APP_ID": {
      "description": "Get this value from https://my.telegram.org"
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org"
      "value": ""
    },
    "BOT_OWNER": {
      "description": "Bot Owner UserID"
    },
    "AUTH_USERS": {
      "description": "allow only pre-defined users to use this bot"
      "value": ""
    },
    "MONGODB_URI": {
      "description": "MongoDB Database URI for Saving UserID for Broadcast. Tutorial: https://www.youtube.com/watch?v=aXlF80Cn7iU"
    },
    "TIME_LIMIT": {
      "description": "Set timeout for subprocess (In seconds)",
      "value": "0"
    },
    "BROADCAST_AS_COPY": {
      "description": "Broadcast as Copy or with Forward Tag. Value should be True/False."
    },
    "UPDATE_CHANNEL": {
      "description": "For Force Subscribe. Paste your Update channel USERNAME (without @)..",
      "value": "0"
    },
    "LOG_CHANNEL": {
      "description": "To get logs",
      "value": "0"
    },
    "DEF_WATER_MARK_FILE": {
      "description": "Set any name(Optional)",
      "value": "0"
    },
    "DEF_THUMB_NAIL_VID_S": {
      "description": "default thumbnail to be used in the videos. Incase, youtube-dl is unable to find a thumbnail.",
      "value": "0"
    },
    "SCREENSHOTS": {
      "description": "To get Screenshots (True/False)",
      "value": "0"
    }
    "BANNED_USERS": {
      "description": "Banned Unwanted members..",
      "value": "",
      "required": false
    }
    "SESSION_NAME": {
      "description": "Any Bot Session Name."
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
        "options": {
          "version": "12"
    }
   }
  ],
  "buildpacks": [{
    "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  }, {
    "url": "https://github.com/opendoor-labs/heroku-buildpack-p7zip"
  }, {
    "url": "heroku/python"
  }],
  "stack": "container",
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
